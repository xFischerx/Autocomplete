# <?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.user.autocomplete.MainActivity">

    <AutoCompleteTextView
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:id="@+id/minhaLista"
        android:hint="Digite um nome"
        tools:layout_editor_absoluteY="0dp"
        tools:layout_editor_absoluteX="8dp" />

    <EditText
        android:id="@+id/edtIncluir"
        android:layout_width="245dp"
        android:layout_height="43dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="Digite o nome a ser incluido"
        tools:layout_editor_absoluteX="83dp"
        tools:layout_editor_absoluteY="130dp" />

    <EditText
        android:id="@+id/edtExcluir"
        android:layout_width="241dp"
        android:layout_height="39dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:hint="Digite o nome a ser excluido"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="333dp" />

    <Button
        android:id="@+id/btnIncluir"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Incluir"
        tools:layout_editor_absoluteY="196dp"
        tools:layout_editor_absoluteX="161dp"
        android:onClick="Incluir"/>

    <Button
        android:id="@+id/btnExcluir"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Excluir"
        tools:layout_editor_absoluteY="392dp"
        tools:layout_editor_absoluteX="161dp"
        android:onClick="Excluir"/>


</android.support.constraint.ConstraintLayout>
